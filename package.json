{
  "name": "devtrace",
  "displayName": "DevTrace",
  "description": "A Comprehensive VS Code Extension for Advanced Debugging and AI-Assisted fixing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devtrace.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "tsc -b",
    "start": "node backend/server.ts"
  },
  "dependencies": {
    "@types/vscode": "^1.95.0",
    "sqlite3": "^5.1.6",
    "express": "^4.18.2",
    "axios": "^1.4.0",
    "openai": "^4.1.1",
    "xstate": "^4.34.0",
    "@xstate/react": "^1.8.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@types/express": "^4.17.17"
  }
}